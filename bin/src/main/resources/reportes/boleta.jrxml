	<parameter name="nrodoc" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
	e.ape_materno,
	e.ape_paterno,
	e.nombres,
	e.fecha_ingreso,
	e.ape_paterno,
	e.ape_materno,
	e.fecha_nacimiento,
	e.nombres,
	e.nro_documento,
	e.fecha_ingreso,
	daw.tb_cargos.nombre AS nombre_cargo,
	daw.tb_tipo_documentos.nombre AS nombre_tipo_doc,
	daw.tb_sistema_pensiones.nombre AS nombre_sistema_pension,
	daw.tb_tipo_horarios.nombre AS nombre_tipo_horario,
	(
	SELECT
		sum(
			HOUR (
			TIMEDIFF( tb_marcacion.hora_ingreso, tb_marcacion.hora_salida ))) 
	FROM
		tb_marcacion 
	WHERE
		e.id_empleado = tb_marcacion.id_empleado 
		AND tb_marcacion.fecha >= ( SELECT CAST( DATE_FORMAT( NOW(), '%Y-%m-01' ) AS DATE ) ) 
		AND tb_marcacion.fecha <= LAST_DAY( NOW())) AS horas_trabajadas, ( SELECT count( tb_marcacion.id_marcacion ) FROM tb_marcacion WHERE e.id_empleado = tb_marcacion.id_empleado AND tb_marcacion.fecha >= ( SELECT CAST( DATE_FORMAT( NOW(), '%Y-%m-01' ) AS DATE ) ) 
		AND tb_marcacion.fecha <= LAST_DAY(
		NOW()) 
	) AS dias_trabajados,
	(
	SELECT
		ROUND((
				tb_cargos.sueldo /(
					count( a.Date ) * 8 
					) * (
				SELECT
					sum(
						HOUR (
						TIMEDIFF( tb_marcacion.hora_ingreso, tb_marcacion.hora_salida ))) 
				FROM
					tb_marcacion 
				WHERE
					e.id_empleado = tb_marcacion.id_empleado 
					AND tb_marcacion.fecha >= ( SELECT CAST( DATE_FORMAT( NOW(), '%Y-%m-01' ) AS DATE ) ) 
					AND tb_marcacion.fecha <= LAST_DAY(
					NOW())) 
				),
			2 
		) 
	FROM
		(
		SELECT
			last_day( '2022-06-01' ) - INTERVAL (
			a.a + ( 10 * b.a ) + ( 100 * c.a )) DAY AS Date 
		FROM
			(
			SELECT
				0 AS a UNION ALL
			SELECT
				1 UNION ALL
			SELECT
				2 UNION ALL
			SELECT
				3 UNION ALL
			SELECT
				4 UNION ALL
			SELECT
				5 UNION ALL
			SELECT
				6 UNION ALL
			SELECT
				7 UNION ALL
			SELECT
				8 UNION ALL
			SELECT
				9 
			) AS a
			CROSS JOIN (
			SELECT
				0 AS a UNION ALL
			SELECT
				1 UNION ALL
			SELECT
				2 UNION ALL
			SELECT
				3 UNION ALL
			SELECT
				4 UNION ALL
			SELECT
				5 UNION ALL
			SELECT
				6 UNION ALL
			SELECT
				7 UNION ALL
			SELECT
				8 UNION ALL
			SELECT
				9 
			) AS b
			CROSS JOIN (
			SELECT
				0 AS a UNION ALL
			SELECT
				1 UNION ALL
			SELECT
				2 UNION ALL
			SELECT
				3 UNION ALL
			SELECT
				4 UNION ALL
			SELECT
				5 UNION ALL
			SELECT
				6 UNION ALL
			SELECT
				7 UNION ALL
			SELECT
				8 UNION ALL
			SELECT
				9 
			) AS c 
		) a 
	WHERE
		a.Date BETWEEN '2022-06-01' 
		AND last_day( '2022-06-01' ) 
		AND DAYOFWEEK( a.Date ) <> 1 
	ORDER BY
		a.Date 
	) AS sueldo_base,
	ROUND(( SELECT ( sueldo_base )* 9 / 100 ), 2 ) AS aporte_seguro,
	ROUND(( SELECT ( sueldo_base ) * tb_sistema_pensiones.porcentaje_descuento / 100 ), 2 ) AS desc_sistema_pension,
	(
	ROUND(( SELECT ( sueldo_base )* 9 / 100 ), 2 ) + ROUND(( SELECT ( sueldo_base ) * tb_sistema_pensiones.porcentaje_descuento / 100 ), 2 )) AS desc_total,
	( SELECT CASE WHEN e.hijos = 1 THEN 930 ELSE 0 END ) AS asignacion_familiar,
	(
	ROUND(( SELECT ( sueldo_base )* 9 / 100 ), 2 ) + ( SELECT CASE WHEN e.hijos = 1 THEN 930 ELSE 0 END )) AS total_aporte,
	(
	SELECT
		( sueldo_base ) - ROUND(( SELECT ( sueldo_base ) * tb_sistema_pensiones.porcentaje_descuento / 100 ), 2 ) + (
		ROUND(( SELECT ( sueldo_base )* 9 / 100 ), 2 ) + ( SELECT CASE WHEN e.hijos = 1 THEN 930 ELSE 0 END )) 
	) AS total_pagar 
FROM
	daw.tb_empleados AS e
	INNER JOIN daw.tb_cargos ON e.id_cargo = daw.tb_cargos.id_cargo
	INNER JOIN daw.tb_tipo_documentos ON e.id_tipo_documento = daw.tb_tipo_documentos.id_tipo_documento
	INNER JOIN daw.tb_sistema_pensiones ON e.id_sistema_pension = daw.tb_sistema_pensiones.id_sistema_pension
	INNER JOIN daw.tb_tipo_horarios ON e.id_tipo_horario = daw.tb_tipo_horarios.id_tipo_horario 
WHERE
	e.nro_documento = $P{nrodoc}]]>
	</queryString>
